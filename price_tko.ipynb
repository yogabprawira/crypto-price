    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"def get_df_from_ticker(ticker_name, ticker_type, interval) :\\n\",\n",
    "    \"    ticker = get_ticker(ticker_name + 'USDT', ticker_type, interval)\\n\",\n",
    "    \"    df = pd.DataFrame(ticker, columns=[\\n\",\n",
    "    \"    'Open time',\\n\",\n",
    "    \"    'Open',\\n\",\n",
    "    \"    'High',\\n\",\n",
    "    \"    'Low',\\n\",\n",
    "    \"    'Close',\\n\",\n",
    "    \"    'Volume',\\n\",\n",
    "    \"    'Close time',\\n\",\n",
    "    \"    'Quote asset volume',\\n\",\n",
    "    \"    'Number of trades',\\n\",\n",
    "    \"    'Taker buy base asset volume',\\n\",\n",
    "    \"    'Taker buy quote asset volume',\\n\",\n",
    "    \"    'Ignore'])\\n\",\n",
    "    \"    df['Date'] = pd.to_datetime(df['Close time'], unit='ms')\\n\",\n",
    "    \"    df['Open'] = pd.to_numeric(df['Open'], errors='coerce')\\n\",\n",
    "    \"    df['Close'] = pd.to_numeric(df['Close'], errors='coerce')\\n\",\n",
    "    \"    df['High'] = pd.to_numeric(df['High'], errors='coerce')\\n\",\n",
    "    \"    df['Low'] = pd.to_numeric(df['Low'], errors='coerce')\\n\",\n",
    "    \"    df['Volume'] = pd.to_numeric(df['Volume'], errors='coerce')\\n\",\n",
    "    \"    df.set_index('Date', inplace=True)\\n\",\n",
    "    \"    df.sort_index(inplace=True)\\n\",\n",
    "    \"    return df\"\n",
    "   ],\n",
