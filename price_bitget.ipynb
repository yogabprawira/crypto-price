    "   \"outputs\": [],\n",
    "   \"execution_count\": null\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {},\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": \"df_btc_usdt = get_df_from_ticker('BTC', time_frame, time_from_ms, time_to_ms)\",\n",
    "   \"id\": \"88cf9147fa628d13\",\n",
