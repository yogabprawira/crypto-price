    "  {\n",
    "   \"metadata\": {},\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"def show_chart_rsi(df) :\\n\",\n",
    "    \"    if 'Label' not in df.columns:\\n\",\n",
    "    \"        return\\n\",\n",
    "    \"    width = width_ratio * len(df['Label'])\\n\",\n",
    "    \"    plt.figure(figsize=(width, 6))\\n\",\n",
